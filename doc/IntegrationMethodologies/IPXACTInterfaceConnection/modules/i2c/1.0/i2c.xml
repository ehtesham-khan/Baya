          <spirit:access>read-only</spirit:access>
          <spirit:reset>
            <spirit:value>0x00000000</spirit:value>
          </spirit:reset>
          <spirit:field>
            <spirit:name>reserved</spirit:name>
            <spirit:description>Reserved for future use</spirit:description>
