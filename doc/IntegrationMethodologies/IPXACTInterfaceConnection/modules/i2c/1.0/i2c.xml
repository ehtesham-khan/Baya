            <spirit:bitWidth>24</spirit:bitWidth>
            <spirit:access>read-only</spirit:access>
          </spirit:field>
          <spirit:field>
            <spirit:name>Data_byte</spirit:name>
            <spirit:description>Bit 7 is the MSB</spirit:description>
